<div class="container">
{{ form_start(form, {attr: {'novalidate': 'novalidate'}}) }}
    {{ form_row(form.email) }}
    {{ form_row(form.password) }}
    {{ form_row(form.company) }}      
    {{ form_row(form.job) }}      
    {{ form_row(form.firstName) }}
    {{ form_row(form.lastName) }}
    {{ form_row(form.address) }}
    {{ form_row(form.zip) }}
    {{ form_row(form.city) }}
    {{ form_row(form.phone) }}
    <hr>
    <button class="btn btn-lg btn-success" type="submit">Enregistrer l'utilisateur</button>
{{ form_end(form) }}
</div>